{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "startCommand": "cd admin-panel/admin-backend/backend && java -Xmx512m -Dserver.port=$PORT -jar target/admin-panel-backend-1.0.0.jar",
    "healthcheckPath": "/",
    "healthcheckTimeout": 300,
    "restartPolicyType": "ON_FAILURE"
  }
}
